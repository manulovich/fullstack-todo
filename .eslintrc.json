{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": "airbnb-base/legacy",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-var": "error",
        "indent": ["error", 4],
        "no-console": ["error", { "allow": ["warn", "error", "log"] }]
    }
}
